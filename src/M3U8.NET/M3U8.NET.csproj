<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
  	<PackageId>M3U8Parser</PackageId>
  	<Version>1.0.0</Version>
  	<Authors>BNeto</Authors>
  	<Description>A library for parsing and validating M3U8.</Description>
  	<PackageTags>m3u8; hls; iptv; parser</PackageTags>
  	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

</Project>
